.readymade-classification-interface
  display: table
  table-layout: fixed
  width: 100%

  .readymade-subject-viewer-container
    background: rgba(black, 0.5)
    border-radius: 5px
    display: table-cell
    overflow: hidden
    padding: 1em
    text-align: center
    vertical-align: middle
    width: 75%

  .readymade-decision-tree-container
    display: table-cell
    vertical-align: middle
    width: 25%
